{
    "name": "deloitte-react-material-ui",
    "version": "1.0.0",
    "description": "Boilerplate for quick-starting React / Material UI applications that have a NodeJS back-end.",
    "main": "server.js",
    "scripts": {
        "postinstall": "npm install --prefix client",
        "test": "echo \"Error: no test specified\" && exit 1",
        "test:client": "npm test --prefix client",
        "build": "npm run --prefix client/ build",
        "start": "npm run --prefix client/ start",
        "serve": "node server.js",
        "prepare": "husky install",
        "prettier-fix": "./node_modules/.bin/prettier --write \"{src,uiTest*}/**/*.{js,css,scss}\" --config .prettierrc",
        "eslint-check": "./node_modules/eslint/bin/eslint.js --config .eslintrc.json --max-warnings=0 client/src/*.js",
        "prettier-check": "./node_modules/.bin/prettier --check \"{src,uiTest*}/**/*.{js,css,scss}\" --config .prettierrc"
    },
    "repository": {
        "type": "git",
        "url": "https://rmcgeehan_swth@bitbucket.org/delsiuk/quickstart-react-material-ui.git"
    },
    "keywords": [
        "deloitte",
        "react",
        "nodejs",
        "boilerplate",
        "quickstart",
        "client",
        "server"
    ],
    "author": "Ricky McGeehan",
    "license": "ISC",
    "homepage": "https://bitbucket.org/delsiuk/quickstart-react-material-ui",
    "devDependencies": {
        "babel-eslint": "^10.1.0",
        "eslint": "^6.6.0",
        "eslint-config-airbnb": "^18.2.0",
        "eslint-plugin-import": "^2.22.0",
        "eslint-plugin-jsx-a11y": "^6.3.1",
        "eslint-plugin-react": "^7.20.5",
        "eslint-plugin-react-hooks": "^4.0.0",
        "husky": "^5.1.2",
        "prettier": "^2.2.1"
    },
    "dependencies": {
        "express": "^4.17.1",
        "morgan": "^1.10.0",
        "path": "^0.12.7",
        "react-dom": "^17.0.1",
        "react-router-dom": "^5.2.0",
        "winston": "^3.3.3"
    }
}
